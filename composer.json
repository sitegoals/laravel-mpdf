{
    "name": "willmav5000/laravel-mpdf",
    "description": "A mPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "mpdf", "pdf"],
    "authors": [
        {
            "name": "Will Mather",
            "email": "willmav5000@gmail.com"
        },
        {
            "name": "Mohan Raj M",
            "email": "praem1990@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "mpdf/mpdf": "^8.0"
    },

    "autoload": {
        "psr-4": {
            "Willmav5000\\PDF\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Willmav5000\\PDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Willmav5000\\PDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
